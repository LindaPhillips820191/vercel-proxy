{
  "rewrites": [
    { "source": "/image/:url*", "destination": "https://cdn.discordapp.com/:url*" },
     { "source": "/media/:url*", "destination": "https://media.discordapp.net/:url*" },
    { "source": "/chatgpt/:url*", "destination": "https://api.openai.com/:url*" },
    { "source": "/wss/:url*", "destination": "wss://gateway.discord.gg/:url*" } 
  ], 
  "regions": [
        "hkg1"
    ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "headers": [
    {
      "source": "/image/:url*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://gfish.top" },
        { "key": "Access-Control-Allow-Headers", "value": "Origin, X-Requested-With, Content-Type, Accept" }
      ]
    }
  ]
}
